{
  "ApiSettings": {
    "BaseUrl": "https://localhost:7066/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Performance": {
    "EnableLazyLoading": true,
    "EnableServiceWorker": true,
    "CacheStaticAssets": true,
    "DeferNonCriticalServices": true,
    "EnableBrotliCompression": true
  },
  "Globalization": {
    "LoadAllCultures": false,
    "DefaultCulture": "ar-EG",
    "SupportedCultures": [ "en-US", "ar-EG" ]
  }
}